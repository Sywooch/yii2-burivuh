{
    "name": "zabachok/yii2-burivuh",
    "description": "Simple markdown editor for yii2 using filesystem",
    "type": "yii2-extension",
    "keywords": ["documentation","markdown","module"],
    "license": "MIT",
    "minimum-stability" : "dev",
    "authors": [
        {
            "name": "Daniil Romanov",
            "email": "zabachok@zabachok.net"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yetanotherape/diff-match-patch": "*"
    },
    "autoload": {
        "psr-4": {
            "zabachok\\burivuh\\": ""
        }
    },
    "extra": {
        "bootstrap": "zabachok\\burivuh\\Bootstrap"
    }
}
